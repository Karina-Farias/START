algoritmo "Trabalhando com Tipos de Variavéis"

var
   nome: literal
   sobrenome: literal
   nomeCompleto: literal

inicio
      Escreval ("Infome o seu nome: ")
      leia(nome)
      Escreval ("Infome o seu sobrenome: ")
      leia(sobrenome)
      nomeCompleto <- nome + " " + sobrenome
      Escreval (nomeCompleto)
fimalgoritmo
