algoritmo "Escolha"

var
voto: literal
inicio
Escreval("Digite seu voto")
leia(voto)

Escolha voto
caso "20"
     Escreval("Você votou em Karina")
caso "30"
     Escreval("Você votou em Gabriel")
caso "40"
     Escreval("Você votou em Zoe")
outrocaso
     Escreval("Voto nulo")
fimescolha

fimalgoritmo
